{
    "$schema": "https://developer.tempestdx.com/schema/v1/tempest-properties-schema.json",
    "$id": "https://schema.tempestdx.io/privateapps/dashboards/properties.json",
    "type": "object",
    "properties": {
        "id": {
            "title": "ID",
            "type": "string",
            "description": "The unique identifier of the dashboard."
        },
        "name": {
            "title": "Name",
            "type": "string",
            "description": "The name of the dashboard."
        },
        "description": {
            "title": "Description",
            "type": "string",
            "description": "The description of the dashboard."
        },
        "project_id": {
            "title": "Project ID",
            "type": "string",
            "description": "The unique identifier of the project that the dashboard belongs to."
        }
    },
    "required": [
        "id",
        "name",
        "project_id"
    ],
    "additionalProperties": false
}
