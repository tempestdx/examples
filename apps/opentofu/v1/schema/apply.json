{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "description": "The name of the S3 Bucket."
        },
        "versioning": {
            "type": "boolean",
            "description": "The versioning state of the S3 Bucket.",
            "default": false
        },
        "region": {
            "type": "string",
            "description": "The AWS region of the S3 Bucket.",
            "default": "us-east-1"
        }
    },
    "required": [
        "name"
    ],
    "additionalProperties": false
}
